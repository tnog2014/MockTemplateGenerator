	/**
	 * メソッド${methodName}実行時に処理を行わない${className}クラスのモックを返す。
	 *
	 * @param retValue
	 * @return
	 */
	private ${className} getMockWithDoNothing_${className}_${methodName}() {
		// void public
		${className} target = PowerMockito.mock(${className}.class);
		PowerMockito.doNothing().when(target).${methodName}(${objs});
		return target;
	}
